@inproceedings{price15:inter.social.networ,
  keywords = {conference},
  author = {Dominic Price and Derek McAuley and Richard Mortier and Chris
                  Greenhalgh and Michael Brown and Spyros Angelopoulos},
  title = {Inter-Social-Networking: Accounting for Multiple Identities},
  booktitle = {Proceedings of HCI International: Social Computing and Social
                  Media (SCSM)},
  pages = {242--252},
  pagetotal = 11,
  year = 2015,
  month = aug # {~2--7},
  address = {Los Angeles, CA},
  publisher = {Springer International Publishing},
  isbn = {ISBN 978-3-319-20367-6},
  abstract = {We argue that the current approaches to online social
                  networking give rise to numerous challenges regarding the
                  management of the multiple facets of people's digital
                  identities within and around social networking sites (SNS). We
                  propose an architecture for enabling people to better manage
                  their SNS identities that is informed by the way the core
                  Internet protocols developed to support interoperation of
                  proprietary network protocols, and based on the idea of
                  Separation of Concerns [1]. This does not require modification
                  of existing services but is predicated on providing a
                  connecting layer over them, both as a mechanism to address
                  problems of privacy and identity, and to create opportunities
                  to open up online social networking to a much richer set of
                  possible interactions and applications.},
  isbn = {978-3-319-20367-6},
  doi = {10.1007/978-3-319-20367-6_24},
  url = {https://doi.org/10.1007/978-3-319-20367-6_24},
  pdf = {pdf/hcii15-intersocialnteworking.pdf}
}

